{
    "name": "anaxexp/ultima",
    "description": "WordPress Ultimatum Child Theme",
    "type": "wordpress-theme",
    "license": "MIT",
    "homepage": "https://anaxexp.com/",
    "authors": [
        {
            "name": "Christopher Wells",
            "email": "cwells@anaxexp.com",
            "homepage": "https://github.com/anaxexp"
        }
    ],
    "keywords": [
        "wordpress",
        "ultimatum",
        "child-theme"
    ],
    "support": {
        "issues": "https://github.com/anaxexp/altima/issues",
        "forum": "https://discourse.anaxexp.com/"
    },
    "autoload": {
        "psr-4": {
            "Anaxexp\\Ultima\\": "src/lib/Ultima/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5.1",
        "phpmd/phpmd": "^2.4.2"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcs",
            "vendor/bin/phpmd src text cleancode,codesize,controversial,design,naming,unusedcode"
        ]
    }
}
